{
    "name": "adeo-pwa",
    "private": true,
    "version": "9.0.1",
    "description": "Adeo-PWA theme",
    "author": "Magento Commerce",
    "license": "SEE LICENSE IN LICENSE.txt",
    "scripts": {
        "buildpack": "buildpack",
        "build": "yarn generate && yarn run build:prod",
        "build:analyze": "yarn run clean:dist && mkdir dist && webpack -p --profile --no-progress --env.mode production --json > dist/build-stats.json && webpack-bundle-analyzer dist/build-stats.json",
        "build:dev": "yarn run clean:dist && yarn run validate-queries && webpack --no-progress --env.mode development",
        "build:prod": "yarn run clean:dist && webpack --no-progress --env.mode production",
        "clean": "rimraf ./node_modules",
        "clean:dist": "rimraf dist",
        "coveralls": "cat ./coverage/lcov.info | coveralls",
        "download-schema": "graphql get-schema --project adeo-pwa --insecure",
        "lint": "eslint '@(src|scripts)/**/{*.ts,.tsx,.js}'",
        "prettier": "prettier --loglevel warn --write '@(src|scripts)/**/*.@(css|graphql|js|ts|tsx)' '*.js'",
        "prettier:check": "prettier --list-different '@(src|scripts)/**/*.@(css|graphql|js|ts|tsx)' '*.js'",
        "prettier:fix": "yarn run -s prettier -- --write",
        "start": "buildpack serve .",
        "start:debug": "node --inspect-brk ./node_modules/.bin/webpack-dev-server --progress --color --env.mode development",
        "test": "jest",
        "test:ci": "node --experimental-worker node_modules/.bin/jest --max-workers=3 --ci --reporters=default --reporters=jest-junit --outputFile=report-junit.xml",
        "test:debug": "node --experimental-worker --inspect-brk node_modules/.bin/jest --no-cache",
        "test:dev": "node --experimental-worker node_modules/.bin/jest --watch",
        "validate-queries": "yarn run download-schema && graphql validate-magento-pwa-queries --project adeo-pwa",
        "watch": "webpack-dev-server --progress --color --env.mode development",
        "storybook": "start-storybook -p 9001 -c src/.storybook",
        "storybook:build": "build-storybook -c src/.storybook -o storybook-dist",
        "generate": "graphql-codegen -r dotenv/config"
    },
    "main": "src/lib/index.js",
